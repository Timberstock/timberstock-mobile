{
  "expo": {
    "name": "TimberStock",
    "slug": "timberstock-mobile-app",
    "version": "1.1.22",
    "scheme": "timberstock",
    "orientation": "portrait",
    "icon": "./assets/dallelogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/dallesplashart.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/77c3e023-bb35-4869-91e8-aef0a6820e4c",
      "requestHeaders": {
        "expo-channel-name": "staging"
      }
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "22",
      "googleServicesFile": "./firebase_services_files/GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.atc.timberstockapp"
    },
    "android": {
      "versionCode": 22,
      "googleServicesFile": "./firebase_services_files/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/dallelogo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.atc.timberstockandroid",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/dallelogo.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@config-plugins/react-native-pdf",
      "@config-plugins/react-native-blob-util",
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "77c3e023-bb35-4869-91e8-aef0a6820e4c"
      }
    },
    "runtimeVersion": {
      "policy": "nativeVersion"
    }
  }
}
