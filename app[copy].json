{
  "expo": {
    "name": "TimberStock",
    "slug": "timberstock-mobile-app",
    "version": "0.0.9", // new builds change this
    "orientation": "portrait",
    "icon": "./assets/dallelogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/dallesplashart.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "googleServicesFile": "./SpecificPlatformFiles/ios/GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.atc.timberstockapp",
      "useFrameworks": "static", // got from https://rnfirebase.io/#configure-react-native-firebase-modules
      "buildNumber": "0.11" // new builds change this
    },
    "android": {
      "googleServicesFile": "./SpecificPlatformFiles/android/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/dallelogo.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 5, // new builds change this
      "package": "com.atc.timberstockapp",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/dallelogo.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/firestore"
      // https://github.com/invertase/react-native-firebase/issues/6990
      // https://github.com/invertase/react-native-firebase/pull/7502/files
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "77c3e023-bb35-4869-91e8-aef0a6820e4c"
      }
    }
  }
}